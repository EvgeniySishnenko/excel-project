{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": "off",
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "no-debugger": "off",
    "arrow-parens":"off",
    "indent": "off",
    "space-before-blocks":"off",
    "keyword-spacing": "off",
    "max-len": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
